{
  "name": "uikit",
  "version": "0.0.1",
  "description": "Tapucom React Native Uikit",
  "scripts": {
    "build": "npm run test && rm -rf ./dist/** && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && npm run copy-assets",
    "copy-assets": "cp -R src/Assets/Images dist/Assets/",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "watch": "tsc -- --watch",
    "test": "jest --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts*'",
    "storybook": "npx -p @storybook/cli sb init --type react_native"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tapucomtech/uikit"
  },
  "bugs": {
    "url": "https://github.com/tapucomtech/uikit/issues"
  },
  "homepage": "https://github.com/tapucomtech/uikit",
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "keywords": [
    "react",
    "react-native",
    "uikit"
  ],
  "author": "TapucomTech <info@tapu.com>",
  "license": "Apache",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/addon-react-native-web": "^0.0.18",
    "@storybook/react-native": "^5.3.25",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-native": "^0.69.5",
    "@types/styled-components": "^5.1.25",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-native-web": "^0.18.7",
    "jest": "^28.1.3",
    "react": "^18.0.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.69.3",
    "react-native-web": "^0.18.7",
    "styled-components": "^5.3.5",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-native": "^0.69.3"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
