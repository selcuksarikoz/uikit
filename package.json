{
  "name": "uikit",
  "version": "0.0.1",
  "description": "Tapucom React Native Uikit",
  "scripts": {
    "build": "npm run test && rm -rf ./dist/** && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && npm run copy-assets",
    "copy-assets": "cp -R src/Assets/Images dist/Assets/",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "watch": "tsc -- --watch",
    "test": "jest --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts*'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tapucomtech/uikit"
  },
  "bugs": {
    "url": "https://github.com/tapucomtech/uikit/issues"
  },
  "homepage": "https://github.com/tapucomtech/uikit",
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "keywords": [
    "react",
    "react-native",
    "uikit"
  ],
  "author": "TapucomTech <info@tapu.com>",
  "license": "Apache",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-native": "^0.69.5",
    "@types/styled-components": "^5.1.25",
    "jest": "^28.1.3",
    "react": "^18.0.0",
    "react-native": "^0.69.3",
    "styled-components": "^5.3.5",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-native": "^0.69.3"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
